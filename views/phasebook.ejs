<% const icon = "data:image/jpeg;base64," %>
<%- include("partials/cdns.ejs") %>	
	<link rel="stylesheet" type="text/css" href="phasebook.css">
	<title>oldBOOK</title>
</head>
<body>
	<% userid = null %>
	<!-- NAVBAR -->
	<nav class="navbar fixed-top navbar-dark bg-dark">
  		<div class="container-md">
	  		<a class="navbar-brand" href=""><span class="brand icon"> <img src="<%= icon %>">ldBOOK</span></a>
			<form class="nav-form" action="/signin" method="post">
				<div class="row justify-content-between">
					<div class="col col-4 ">
						<div>Email</div>
						<div><input class="form" type="email" name = "useremail" required></div>
					</div>
					<div class="col col-4">
						<div>Password</div>
						<div><input class="form" type="password" name="userpassword" required></div>
						<a href="" class="forgotten">Forgotten account?</a>
					</div>
					<div class="col col-3 align-self-center">
						<button type="submit" class="login-btn">Log In</button>
					</div>
				</div>
			</form>
	  	</div>
	</nav>

	<!-- FORM REGION -->

	<div class="container-md">
		<div class="row">
			<!-- <div class="col col-6">
				<h4 class="left-text">oldBOOK helps you connect and share with the people in your life.</h4>	
			</div> -->
			<div class="col col-5 pl-5 text-center m-auto">
				<form class="row" action="/signup" method="post">
					<div>
						<h1>Create an account</h1>
						<div>It's quick and easy.</div>
					</div>
					<div class="row">
						<div>
							<div class="input-group mt-3">
							  	<input type="text" placeholder="Enter name" name="username" aria-label="Name" class="form-control rounded mr-2" required>
							  	<input type="text" placeholder="Surname" name="usersurname" aria-label="Surname" class="form-control rounded" required>
							</div>
							<div class="input-group mt-3">
							  	<input type="email" name="useremail" placeholder="Email address" class="form-control rounded" required>
							</div>
							<div class="input-group mt-3">
							  	<input type="password" placeholder="New password" name="userpassword" aria-label="password" class="form-control rounded" required>
							</div>
							<div class="input-group mt-3">
							  	<label class="label-bday w-100">Birthday:
							  		<div>
							  			<input type="date" aria-label="birthday" name="userbirthday" class="form-control d-inline w-75" required>
							  			<!-- <abbr title="Click for more information"><i class="fas fa-question-circle bday-info"></i></abbr> -->
							  		</div>
							  	</label>
							</div>
							<div class="input-group mt-3">
							  	<label class="label-gender w-100">Gender:<br>
							  		<input type="radio" name="usergender" value="female" class="ml-3  mr-1 mt-2"><span>Female</span>
  									<input type="radio" name="usergender" value="male" class="ml-3 mr-1 "><span>Male</span>
  									<input type="radio" name="usergender" value="other" class="ml-3 mr-1 "><span>Other</span>
  									<!-- <abbr title="Click for more information" class="ml-3"><i class="fas fa-question-circle gender-info"></i></abbr> -->
							  	</label>
							</div>
							<div class="agreement-text text-center mt-2">
								<p>By clicking Sign Up, you agree to our <a href="#">Terms</a>, <a href="#">Data Policy</a> and <a href="#">Cookie Policy</a>.</p>
							</div>
							<div>
								<button type="submit" class="btn btn-sm w-50 btn-success mb-4">Sign up</button>
							</div>
							<!-- <div class="create-text">
								<p><a href="#">Create a Page</a> for a celebrity, band or business.</p>
							</div> -->
						</div>
					</div>
				</form>
			</div>
			</div>	

			<% function dispReview(author, Url, content){%>
				<div class="jumbotron jumbotron-sm review"> 
					<i class="fas fa-check-circle cross"></i>
					<div class="author">
						<img src="<%= Url %>">
						<h4 class="d-inline"><%= author %></h4>
					</div>
					<p>~ <%= content %></p>
				</div>
			<% } %>	
			<% function dispHidden(){%>
				<div class="jumbotron jumbotron-sm review"> 
					<i class="far fa-check-circle cross"></i>
					<p class="display-5">This review is hidden until admin approves it!, stay tuned... </p>
				</div>
			<% } %>	

			<% if(reviews){ %>
				<h1 >Featured Reviews</h1>
				<p>(only reviews that follows community guidelines are posted and requires admin's approval)</p>
				<%  reviews.forEach(function(review){ 
						if(review.featured == "true"){ 
							dispReview(review.author, review.url, review.content);
						}
						else{ 
								dispHidden();
						}
		 			})
				}	 %>
	</div>
	<%- include("partials/footer.ejs") %>